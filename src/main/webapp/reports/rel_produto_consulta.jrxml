	<parameter name="CODIGO_PARAMETRO" class="java.lang.String"/>
	<queryString language="SQL">
		<![CDATA[SELECT produto.id,
	produto.descricao,
	produto.preco,
	produto.quantidade,
	fornecedor.razaosocial,
	fornecedor.cnpj
FROM produto
	INNER JOIN fornecedor ON 
	 produto.id_fornecedor = fornecedor.id 
WHERE 
	 produto.id = $P!{CODIGO_PARAMETRO} 
ORDER BY produto.id ASC]]>
	</queryString>
