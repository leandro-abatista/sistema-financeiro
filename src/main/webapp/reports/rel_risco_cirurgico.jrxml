	<queryString language="SQL">
		<![CDATA[SELECT riscocirurgico.id,
	riscocirurgico.acv,
	riscocirurgico.antecedentespessoais,
	riscocirurgico.ar,
	riscocirurgico.celular,
	riscocirurgico.criterioslee,
	riscocirurgico.dataavaliacao,
	riscocirurgico.datanascimento,
	riscocirurgico.ecocardiograma,
	riscocirurgico.eletrocardiograma,
	riscocirurgico.email,
	riscocirurgico.estadogeral,
	riscocirurgico.examehematologico,
	riscocirurgico.idade,
	riscocirurgico.indicegoldman,
	riscocirurgico.medicamentoemuso,
	riscocirurgico.nome,
	riscocirurgico.observacao,
	riscocirurgico.paciente,
	riscocirurgico.pelemucosas,
	riscocirurgico.raioxtorax,
	riscocirurgico.riscocirurgicograu,
	riscocirurgico.sexo,
	riscocirurgico.ta,
	riscocirurgico.tabelaasa,
	riscocirurgico.tabelanyha,
	riscocirurgico.cpf,
	cirurgia.descricao,
	endereco.cep,
	endereco.bairro,
	endereco.complemento,
	endereco.logradouro,
	endereco.numero,
	cidade.nome,
	estado.sigla
FROM riscocirurgico
	INNER JOIN cirurgia ON 
	 riscocirurgico.id_cirurgia = cirurgia.id 
	INNER JOIN endereco ON 
	 riscocirurgico.id_endereco = endereco.id 
	INNER JOIN cidade ON 
	 endereco.id_cidade = cidade.id 
	INNER JOIN estado ON 
	 cidade.id_estado = estado.id]]>
	</queryString>
